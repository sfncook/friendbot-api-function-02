{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"]
    },
    {
      "type": "cosmosDB",
      "name": "conversation",
      "direction": "out",
      "databaseName": "keli",
      "containerName": "conversations",
      "connection": "AzureCosmosDBConnection"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}